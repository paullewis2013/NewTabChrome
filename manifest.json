{
    "manifest_version": 3,
    "name": "Paul's New Tab Page",
    "version": "1.0.0",
    "description": "Paul's New Tab Page",
    "author": "Paul Lewis",
    "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    "action" : {
        "default_icon": "imgs/icon128.png",
        "default_popup": "popup.html"  
    },
    "permissions": [
        "storage"
    ],
    "icons": {
      "16": "imgs/icon16.png",
      "128": "imgs/icon128.png"
    }
  }